	</Page.Resources>
	<StackPanel>
		<TextBox
			x:Name="Email"
			Width="200"
			HorizontalAlignment="Center"
			VerticalAlignment="Center"
			Header="Email"
			wasm:Style="{StaticResource WasmMaterialOutlinedTextBoxStyle}"
			PlaceholderText="Email" />
		<TextBox
			x:Name="Email1"
			Width="200"
			HorizontalAlignment="Center"
			VerticalAlignment="Center"
			Header="Email"
			Style="{StaticResource MaterialOutlinedTextBoxStyle}"
			PlaceholderText="Email" />
		<PasswordBox
			x:Name="Password"
			Width="200"
			HorizontalAlignment="Center"
			VerticalAlignment="Center"
			Header="Password"
			PlaceholderText="Password" />
		<PasswordBox
			x:Name="Password1"
			Width="200"
			HorizontalAlignment="Center"
			VerticalAlignment="Center"
			Header="Password"
			Style="{StaticResource MaterialFilledPasswordBoxStyle}"
			PlaceholderText="Password" />
		<PasswordBox
			x:Name="Password2"
			Width="200"
			Style="{StaticResource MaterialOutlinedPasswordBoxStyle}"
			HorizontalAlignment="Center"
			VerticalAlignment="Center"
			Header="Password"
			PlaceholderText="Password" />
		<Button x:Name="Submit" Content="Submit" />
	</StackPanel>
</Page>
